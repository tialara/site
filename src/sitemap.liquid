---
layout: null
permalink: /sitemap.xml
---
{{ '<?xml version="1.0" encoding="UTF-8"?>' }}
<urlset xmlns='http://www.sitemaps.org/schemas/sitemap/0.9'>
  {%- for page in collections.all -%}
    {%- unless page.data.sitemap == false or page.url == '/404.html' -%}
      <url>
        <loc>
          {{- site.baseUrl -}}
          {{- page.url | url | replace: 'index.html', '' -}}
        </loc>
        <lastmod>{{ page.date | date: '%Y-%m-%d' }}</lastmod>
        <changefreq>
          {%- if page.data.changefreq %}{{ page.data.changefreq }}{% else %}monthly{% endif -%}
        </changefreq>
        <priority>
          {%- if page.data.priority %}{{ page.data.priority }}{% else %}0.5{% endif -%}
        </priority>
      </url>
    {%- endunless -%}
  {%- endfor -%}
</urlset>
